{
    "name": "nodejs-weather-app",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn src/app.ts",
        "start": "node dist/index.js",
        "build": "tsc -p tsconfig.json --pretty",
        "inst_node": "npm i @types/node --save-dev; npm i node",
        "inst_request": "npm i @types/request --save-dev; npm i request",
        "install": "npm run inst_node; npm run inst_request"
    },
    "authors": "Joshua Loftin",
    "dependencies": {
        "node": "^17.7.2",
        "request": "^2.88.2"
    },
    "devDependencies": {
        "@types/node": "^17.0.42",
        "@types/request": "^2.48.8"
    }
}
